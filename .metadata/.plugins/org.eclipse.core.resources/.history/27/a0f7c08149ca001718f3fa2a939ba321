
public class Bitshifter {

	private int shifter;
	
	public static String encrypt(String key) {
		String result = "";
		int keySize = key.length();
		char ch;
		
//		if (keySize > 239) {
//			
//			while (keySize > 32) {
//				
//			}
//		}
//		
		for (int i = 0; i < keySize; i++ ) {
			ch = key.charAt(i);
			ch += keySize;
			result += ch;
		}
		
		return result;
	}
	
	public static String decrypt(String key) {
		String result = "";
		int keySize = key.length();
		
		return null;
		
	}
	
	
}